{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "generators": true,
      "modules": true,
      "objectLiteralDuplicateProperties": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "no-underscore-dangle": 0,
    "no-throw-literal": 0,
    "no-new-wrappers": 0,
    "no-bitwise": ["error", { "int32Hint": true }]

  }
}
